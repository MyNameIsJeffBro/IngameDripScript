-- Know IF Horns Are On Or Off

local HornsOnSV = Instance.new("StringValue", workspace)
HornsOnSV.Name = "HornsOnSV"
HornsOnSV.Value = "Off"

-- Horns Coroutine

local Horns = coroutine.create(function()

	while true do

		local plr = game.Players.LocalPlayer
		local chr = plr.Character

		-- Creating The Hat Part's

		local HornsAcc = Instance.new("Accessory", chr)			
		local Handle = Instance.new("Part")		
		local HatAttachment = Instance.new("Attachment")		
		local Fire = Instance.new("Fire", HatAttachment)	
		local Mesh = Instance.new("SpecialMesh", Handle)	
		local AccessoryWeld = Instance.new("Weld", Handle)	


		HornsAcc.Name = "KorbloxHorns"		
		HornsAcc.AttachmentForward = Vector3.new(0,0,-1)		
		HornsAcc.AttachmentPos = Vector3.new(0,-0.3,0)		
		HornsAcc.AttachmentRight = Vector3.new(1,0,0)		
		HornsAcc.AttachmentUp = Vector3.new(0,1,0)

		-- Need's For the horns


		Handle.Parent = chr.KorbloxHorns	
		Handle.Name = "Handle"			
		Handle.Size = Vector3.new(1.23, 0.76, 1.41)		
		--Handle.CFrame = chr.Head:FindFirstChild("HatAttachment").CFrame
		--Handle.CFrame = chr.Head.CFrame+Vector3.new(0,1.5,0)			
		Handle.CanCollide = false			
		Handle.CanQuery = false		
		Handle.CanTouch = false		
		Handle.Anchored = false

		-- AttachMent's


		HatAttachment.Parent = HornsAcc.Handle		
		HatAttachment.Name = "HatAttachment"		
		HatAttachment.Visible = false
		HatAttachment.Position = Vector3.new(0, 0.7, 0)
		HatAttachment.Axis = Vector3.new(1,0,0)
		HatAttachment.SecondaryAxis = Vector3.new(0,1,0)
		--HatAttachment.WorldAxis = Vector3.new(-0.775,-0,-0.632)
		HatAttachment.Orientation = Vector3.new(-0,-0,-0)
		--HatAttachment.WorldPosition = Vector3.new(-99.713,4.935,5.504)
		-- HatAttachment.WorldSecondaryAxis = Vector3.new(0.024,0.999,-0.03)

		-- Particle's


		Fire.Name = "Fire"			
		Fire.Color = Color3.new(106, 197, 236)			
		Fire.Enabled = true			
		Fire.Heat = 8			
		Fire.SecondaryColor = Color3.new(26, 198, 198)			
		Fire.Size = 3			
		Fire.TimeScale = 1

		-- Mesh Part's


		Mesh.Name = "Mesh"			
		Mesh.MeshId = "http://www.roblox.com/asset/?id=215680403"			
		Mesh.MeshType = Enum.MeshType.FileMesh		
		Mesh.Offset = Vector3.new(0,1.35,0)
		Mesh.Scale = Vector3.new(1,1,1)			
		Mesh.TextureId = "rbxassetid://1679768518"		
		Mesh.VertexColor = Vector3.new(1,1,1)

		-- Weld's

		AccessoryWeld.Name = "AccessoryWeld"			
		AccessoryWeld.Part0 = Handle				
		AccessoryWeld.Part1 = chr.Head		
		AccessoryWeld.Enabled = true

		--[[local newweld = Instance.new("Weld", Handle)
		newweld.Name = "newweld"
		newweld.Part0 = Mesh
		newweld.Part1 = Handle
		AccessoryWeld.Enabled = true--]]


		coroutine.yield()
	end
end)


-- Know IF Horns Are On Or Off

local BlackIronHornsOnSV = Instance.new("StringValue", workspace)
BlackIronHornsOnSV.Name = "BlackIronHornsOnSV"
BlackIronHornsOnSV.Value = "Off"

-- Black Iron Horns coroutine

local BlackIronHorns = coroutine.create(function()

	while true do

		local plr = game.Players.LocalPlayer
		local chr = plr.Character

		--Creating Part's For the horns

		local BlackIronHorns = Instance.new("Accessory", chr)
		local Handle = Instance.new("Part", BlackIronHorns)
		local HatAttachment = Instance.new("Attachment", Handle)
		local Mesh = Instance.new("SpecialMesh", Handle)
		local OriginalSize = Instance.new("Vector3Value", Handle)
		local AccessoryWeld = Instance.new("Weld", Handle)

		--Horns

		BlackIronHorns.Name = "BlackIronHorns"
		BlackIronHorns.AttachmentForward = Vector3.new(0,0,-1)
		BlackIronHorns.AttachmentPos = Vector3.new(0,0.34,0.25)
		BlackIronHorns.AttachmentRight = Vector3.new(1,0,0)
		BlackIronHorns.AttachmentUp = Vector3.new(0,1,0)

		-- Handle

		Handle.Parent = BlackIronHorns
		Handle.Name = "Handle"
		Handle.Size = Vector3.new(1.6, 0.8, 1.6)
		Handle.CanCollide = false			
		Handle.CanQuery = false		
		Handle.CanTouch = false		
		Handle.Anchored = false

		-- HatAttachment

		HatAttachment.Parent = BlackIronHorns.Handle		
		HatAttachment.Name = "HatAttachment"		
		HatAttachment.Visible = false
		HatAttachment.Position = Vector3.new(0, 0.7, 0)
		HatAttachment.Axis = Vector3.new(1,0,0)
		HatAttachment.SecondaryAxis = Vector3.new(0,1,0)
		--HatAttachment.WorldAxis = Vector3.new(-0.775,-0,-0.632)
		HatAttachment.Orientation = Vector3.new(0,0,0)
		--HatAttachment.WorldPosition = Vector3.new(-99.713,4.935,5.504)
		-- HatAttachment.WorldSecondaryAxis = Vector3.new(0.024,0.999,-0.03)

		-- The Mesh Of The Horns

		Mesh.Name = "Mesh"
		Mesh.MeshId = "rbxassetid://627540549"			
		Mesh.MeshType = Enum.MeshType.FileMesh		
		Mesh.Offset = Vector3.new(0,0.83,0)
		Mesh.Scale = Vector3.new(1.7,1.7,1.7)			
		Mesh.TextureId = "rbxassetid://627529830"		
		Mesh.VertexColor = Vector3.new(1,1,1)

		-- The Weld

		AccessoryWeld.Parent = Handle
		AccessoryWeld.Name = "AccessoryWeld"			
		AccessoryWeld.Part0 = Handle				
		AccessoryWeld.Part1 = chr.Head		
		AccessoryWeld.Enabled = true

		coroutine.yield()

	end
end)




