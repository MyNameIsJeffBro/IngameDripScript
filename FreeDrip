local UserInputService = game:GetService("UserInputService")


UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)

	--if input.UserInputType == Enum.UserInputType.Keyboard then

	if input.KeyCode == Enum.KeyCode.V then

		print("You Pressed V!")
		HeadLessUI.Enabled = not HeadLessUI.Enabled

	end
end)

TextButton.MouseButton1Click:Connect(function()

	local plr = game.Players.LocalPlayer
	local Character = plr.Character

	local Head = Character:FindFirstChild("Head")
	local PlrHead = Character
	local Face = Head:GetDescendants()

	print(Face)

	if PlrHead:FindFirstChild("Head").Transparency == 1 and Head.face.Transparency == 1 then

		PlrHead:FindFirstChild("Head").Transparency = 0
		Head.face.Transparency = 0

	else

		PlrHead:FindFirstChild("Head").Transparency = 1
		Head.face.Transparency = 1

	end
end)	

TextButton_1.MouseButton1Click:Connect(function() -- Korblox Right Leg

	local plr = game.Players.LocalPlayer
	local chr = plr.Character

	if chr.RightFoot.Transparency == 0 then

		chr.RightFoot.Transparency = 1

		chr.RightLowerLeg.Transparency = 1

		chr.RightUpperLeg.MeshId = "https://assetdelivery.roblox.com/v1/asset/?id=9598310133"
		chr.RightUpperLeg.TextureID = "https://www.roblox.com/asset/?id=902843398"

	else

		chr.RightFoot.Transparency = 0

		chr.RightLowerLeg.Transparency = 0

		chr.RightUpperLeg.MeshId = StringRVal.Value
		chr.RightUpperLeg.TextureID = StringRVal1.Value
	end
end)

TextButton_2.MouseButton1Click:Connect(function() -- Korblox Left Leg

	local plr = game.Players.LocalPlayer
	local chr = plr.Character

	if chr.LeftFoot.Transparency == 0 then

		chr.LeftFoot.Transparency = 1

		chr.LeftLowerLeg.MeshId = "https://assetdelivery.roblox.com/v1/asset/?id=9598310137"
		chr.LeftLowerLeg.TextureID = "https://www.roblox.com/asset/?id=902842271"

		chr.LeftUpperLeg.MeshId = "https://assetdelivery.roblox.com/v1/asset/?id=9598310131"
		chr.LeftUpperLeg.TextureID = "https://www.roblox.com/asset/?id=902842271"

	else

		chr.LeftFoot.Transparency = 0

		chr.LeftLowerLeg.MeshId = StringLVal.Value
		chr.LeftLowerLeg.TextureID = StringLVal1.Value

		chr.LeftUpperLeg.MeshId = StringLVal3.Value
		chr.LeftUpperLeg.TextureID = StringLVal4.Value

	end
end)

TextButton_3.MouseButton1Click:Connect(function()-- Korblox Horns

	-- Get Plr And Character

	local plr = game.Players.LocalPlayer
	local chr = plr.Character


	-- Disable Player Control's So Horns Stick In Right Place
	--	local pm = require(game:GetService("Players").LocalPlayer.PlayerScripts:WaitForChild("PlayerModule"))
	--	local FreezeCmd = pm:GetControls()

	if workspace.HornsOnSV.Value == "Off" then

		--zFreezeCmd:Disable()
		--wait(0.2)
		print(coroutine.status((Horns)))
		--Horns()
		coroutine.resume(Horns)
		print(coroutine.status(Horns))
		workspace.HornsOnSV.Value = "On"
		--FreezeCmd:Enable()
	else

		chr:FindFirstChild("KorbloxHorns"):Destroy()
		workspace.HornsOnSV.Value = "Off"

	end
end)

TextButton_4.MouseButton1Click:Connect(function() -- Black Iron Horns

	-- Get Plr And Character

	local plr = game.Players.LocalPlayer
	local chr = plr.Character

	if workspace.BlackIronHornsOnSV.Value == "Off" then
	    
	    print(coroutine.status(BlackIronHorns))
		coroutine.resume(BlackIronHorns)
		print(coroutine.status(BlackIronHorns))
		workspace.BlackIronHornsOnSV.Value = "On"
	else
		chr:FindFirstChild("BlackIronHorns"):Destroy()
		workspace.BlackIronHornsOnSV.Value = "Off"
	end

end)
